{
  "name": "@vibe-anywhere/cli",
  "version": "3.1.0",
  "type": "module",
  "description": "Vibe Anywhere CLI helper for managing workspaces",
  "main": "dist/index.js",
  "bin": {
    "vibe-anywhere": "dist/index.js"
  },
  "scripts": {
    "build": "tsc && npm run bundle",
    "bundle": "esbuild dist/index.js --bundle --platform=node --format=esm --banner:js='#!/usr/bin/env node' --outfile=dist/vibe-anywhere --external:node:* --external:fs --external:path --external:url",
    "dev": "tsc --watch"
  },
  "keywords": [
    "vibe-anywhere",
    "cli",
    "environment-variables"
  ],
  "author": "Vibe Anywhere",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^22.0.0",
    "esbuild": "^0.27.2",
    "typescript": "^5.5.0"
  }
}
