{
  "name": "@session-hub/cli",
  "version": "1.8.3",
  "type": "module",
  "description": "Session Hub CLI helper for managing workspaces",
  "main": "dist/index.js",
  "bin": {
    "session-hub": "dist/index.js"
  },
  "scripts": {
    "build": "tsc && npm run bundle",
    "bundle": "esbuild dist/index.js --bundle --platform=node --format=esm --banner:js='#!/usr/bin/env node' --outfile=dist/session-hub --external:node:* --external:fs --external:path --external:url",
    "dev": "tsc --watch"
  },
  "keywords": [
    "session-hub",
    "cli",
    "environment-variables"
  ],
  "author": "Session Hub",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^22.0.0",
    "esbuild": "^0.27.2",
    "typescript": "^5.5.0"
  }
}
